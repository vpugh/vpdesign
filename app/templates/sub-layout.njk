<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<title>{% block title %}{% endblock %}</title>
	{% block css %}
	<!--build:css css/styles.min.css-->
	<link rel="stylesheet" href="../css/main.css" title="" type="text/css" media="screen" charset="utf-8">
	<!--endbuild-->
	<!-- Calluna Sans from TypeKit -->
	<script src="https://use.typekit.net/qie0vrf.js"></script>
	<script>try{Typekit.load({ async: true });}catch(e){}</script>
	{% endblock %}
</head>
<body class="{{activePage}}">
	{% include "partials/header.njk" %}
	{% block jumbotron %}{% endblock %}
	{% block content %}{% endblock %}
	{% include "partials/contact.njk" -%}
	{% include "partials/footer.njk" -%}
	<a href="#0" class="sttop"><span class="sr-only">Back to Top</span>Top</a>
	{% block js %}
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
	<!--build:js js/main.min.js -->
	<script src="../js/script.js"></script>
	<script src="../js/main.js"></script>
	<!-- endbuild -->
	{% block heroPattern %}{% endblock -%}
    {% block typeformJS -%}
    <script>(function(){var qs,js,q,s,d=document,gi=d.getElementById,ce=d.createElement,gt=d.getElementsByTagName,id='typef_orm_share',b='https://s3-eu-west-1.amazonaws.com/share.typeform.com/';if(!gi.call(d,id)){js=ce.call(d,'script');js.id=id;js.src=b+'share.js';q=gt.call(d,'script')[0];q.parentNode.insertBefore(js,q)}})()</script>
